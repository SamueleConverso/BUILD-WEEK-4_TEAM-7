@model Error404

@{
    ViewData["Title"] = "Error";
}

    <div class="d-flex flex justify-content-center mb-2">
        <img src="data:image/png;base64," alt="404 Image" class="w-full h-full object-cover">
    </div>
    <div class="flex flex-col justify-center items-center text-center p-6">
        <h1 class="text-5xl fw-bold text-white mb-4">Page Not Found</h1>
        <p class="fs-3 text-white mb-6">
            Siamo spiacenti, ma la pagina che stai cercando non esiste. Potrebbe essere stata rimossa, aver cambiato nome o non essere disponibile temporaneamente.
        </p>
    <a href="/" class="btn btn-home2 btn-lg text-white mt-4">
        Torna alla Home
    </a>
     </div>
